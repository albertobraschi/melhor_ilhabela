<% form_for [:admin,@estabelecimento,@foto], :html => { :multipart => true } do |f| %>
  <%= f.error_messages %>
  <ul>
    <li>
      <div class="input-block-50 center">
        <%= f.label :imagem,"Escolha a imagem" %><br />
        <%= f.file_field :imagem %>
      </div>
      <div class="input-block-50 center">
        <%= f.label :imagem_legenda,"Legenda para imagem" %><br />
        <%= f.text_field :imagem_legenda %>
      </div>
    </li>
    <li><br style="clear:both" /><div class="float_left"><button type="submit" name="commit" value="voltar">Salvar e add imagem</button></div></li>
    <li><div class="float_right"><%= f.submit "salvar" %></div></li>
  </ul>
<% end %>

