<% form_for [:admin,@estabelecimento], :html=>{:class=>"validate"} do |f| %>
  <%= f.error_messages %>
  <div class="float_right clearfix">
    <%= link_to "Voltar", admin_estabelecimentos_path %>
  </div><br />
  <ul>
    <li>
      <div class="input-block-50 center">
        <%= f.label :tipo_estabelecimento %><br />
        <select id="estabelecimento_tipo_estabelecimento" name="estabelecimento[tipo_estabelecimento]">
          <option value=""></option>
          <option value="BECN">Bares e Casas Noturnas</option>
          <option value="REST">Restaurantes</option>
          <option value="IPLT">Imóveis para Locação</option>
          <option value="HEPO">Hotéis e Pousada</option>
        </select>
      </div>
      <div class="input-block-50 center">
        <%= f.label :cliente_id %><br />
        <%= f.select(:cliente_id, Cliente.find(:all).collect { |item| [item.razao_social,item.id]}, :include_blank => true)%>
      </div>
    </li>
    <li><div class="float_right"><%= f.submit "Continuar" %></div></li>
  </ul>
<% end %>

