<% form_for [:admin,@imovel_locacao] do |f| %>
  <%= f.error_messages %>
    <ul>
    <li>
      <div class="input-block-25">
    <%= f.label :endereco_imovel %><br />
    <%= f.text_field :endereco_imovel %>
      </div>
      <div class="input-block-25">
    <%= f.label :cidade_imovel %><br />
    <%= f.text_field :cidade_imovel %>
      </div>
      <div class="input-block-25">
    <%= f.label :bairro_imovel %><br />
    <%= f.text_field :bairro_imovel %>
      </div>
      <div class="input-block-25">
    <%= f.label :entre_ruas %><br />
    <%= f.text_field :entre_ruas %>
      </div>
    </li>
    <li>
      <div class="input-block-25">
    <%= f.label :ponto_referencia %><br />
    <%= f.text_field :ponto_referencia %>
      </div>
      <div class="input-block-25">
    <%= f.label :valor_aluguel %><br />
    <%= f.text_field :valor_aluguel %>
      </div>
      <div class="input-block-25">
    <%= f.label :valor_condominio %><br />
    <%= f.text_field :valor_condominio %>
      </div>
      <div class="input-block-25">
    <%= f.label :area_total %><br />
    <%= f.text_field :area_total %>
      </div>
    </li>
    <li>
      <div class="input-block-100">
      <%= f.label :descricao %><br />
      <%= f.text_area :descricao, :cols => 80, :rows => 10 %>
      </div>
    </li>
    <li><div class="float_right"><%= f.submit "salvar" %></div></li>
  </ul>
<% end %>

